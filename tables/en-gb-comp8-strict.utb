include latinLetterDef8Dots.uti
include digits6DotsPlusDot6.uti

# Six-dot base patterns cover ASCII ranges 32-63, 96-127 inclusive

space \s 0
punctuation ! 3456
punctuation " 4
sign # 56
sign $ 456
sign % 46
punctuation & 12346
punctuation ' 3
punctuation ( 45
punctuation ) 345
sign * 35
sign + 235
punctuation , 2
punctuation - 36
punctuation . 256
punctuation / 34
digit 0 123456

# symbols 49-57 imported as digits6DotsPlusDot6.uti

punctuation : 25
punctuation ; 23
punctuation < 236
punctuation = 2356
punctuation > 356
punctuation ? 26

sign ` 2346
# Symbols 97-122 imported as part of latinLetterDef8Dots.uti

punctuation { 12356
punctuation | 5
punctuation } 23456
sign ~ 6
sign \x007f 346

# extended 8-dot patterns
# The characters in the specification use the
# CP-437 single-byte character encoding (IBM/DOS)
# The corresponding UTF-8 characters are used here.

# Symbols 0 to 32 are print control characters
sign \x0000 7
sign \x0001 34567
sign \x0002 47
sign \x0003 567
sign \x0004 4567
sign \x0005 467
sign \x0006 123467
sign \x0007 37
sign \x0008 457
# Editor's note: Decided that probably nobody wants to see tab as 3457,
# replacing by space in forward translation
# tabs can be typed in backward translation
nofor sign \x0009 3457
noback space \t 0
# Editor's note: probably nobody wants \r and \n as 357 and 367 respectively
# treating as whitespace
# sign \x000a 357
sign \x000b 2357
sign \x000c 27
# sign \x000d 367
sign \x000e 2567
sign \x000f 347
sign \x0010 1234567
sign \x0011 167
